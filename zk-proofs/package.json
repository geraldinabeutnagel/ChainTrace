{
  "name": "chaintrace-zk-proofs",
  "version": "1.0.0",
  "description": "Zero-knowledge proof circuits and verification for ChainTrace platform",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "compile:circuits": "circom circuits/supplyChainIntegrity.circom --r1cs --wasm --sym",
    "setup:circuits": "node scripts/setupCircuits.js",
    "generate:proof": "node scripts/generateProof.js",
    "verify:proof": "node scripts/verifyProof.js"
  },
  "dependencies": {
    "circomlib": "^2.0.5",
    "snarkjs": "^0.7.0",
    "circomlibjs": "^0.1.7",
    "ffjavascript": "^0.2.0",
    "ethers": "^5.7.2",
    "dotenv": "^16.0.3",
    "winston": "^3.8.2",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^6.1.5",
    "joi": "^17.9.2",
    "axios": "^1.3.4",
    "node-cron": "^3.0.2"
  },
  "devDependencies": {
    "@types/node": "^18.15.11",
    "@types/jest": "^29.5.1",
    "typescript": "^5.0.4",
    "nodemon": "^2.0.21",
    "ts-node": "^10.9.1",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "eslint": "^8.36.0",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "circom": "^2.0.5"
  },
  "keywords": [
    "zero-knowledge-proofs",
    "zk-snarks",
    "circom",
    "privacy",
    "blockchain",
    "supply-chain"
  ],
  "author": "ChainTrace Team",
  "license": "MIT"
}
